{
  "name": "blockwi-qhl",
  "version": "0.9.9",
  "description": "BlockWi-QHL",
  "homepage": ".",
  "main": "electron/main.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "npm run build && electron .",
    "electron-dev": "concurrently \"set BROWSER=none&&npm start\" \"wait-on http://localhost:3000 && set ELECTRON_USE_DEV_SERVER=1&&electron .\"",
    "electron-build": "npm run build && electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "extends": null,
    "appId": "com.blockwi-qhl.app",
    "icon": "public/wokwi_logo_256.png",
    "win": {
      "icon": "public/wokwi_logo_256.png"
    },
    "extraMetadata": {
      "main": "electron/main.js"
    },
    "files": [
      "electron/**/*",
      "build/**/*",
      "!build/ucblockly{,/**/*}"
    ],
    "extraResources": [
      {
        "from": "public/ucblockly",
        "to": "ucblockly"
      }
    ],
    "directories": {
      "output": "release",
      "buildResources": "assets"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "concurrently": "^9.2.1",
    "electron": "^40.4.1",
    "electron-builder": "^26.7.0",
    "react-scripts": "^5.0.1",
    "typescript": "^5.9.3",
    "wait-on": "^9.0.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
